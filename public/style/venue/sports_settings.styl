@charset "utf-8"
@import "../base"

.ww-wrapper
    width 100%
    margin-top 6px
    .wrapper
        width 960px
        margin 0 auto
        background #fafafa
        clear both
        .types-add
            padding 10px 10px 20px
        .types-list
            padding 0 10px
